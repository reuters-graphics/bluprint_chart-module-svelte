    "autoprefixer": "^10.0.2",
    "eslint": "^7.13.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "fa-svelte": "^3.1.0",
    "postcss": "^8.1.7",
    "rollup": "^2.3.4",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
