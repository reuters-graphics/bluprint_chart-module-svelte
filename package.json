{
  "name": "@reuters-graphics/my-chart-module",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "private": false,
  "repository": "github:reuters-graphics/chart-module-my-chart-module",
  "files": [
    "src/js/**/*",
    "src/scss/**/*",
    "badge.svg"
  ],
  "scripts": {
    "chart:dev": "sapper dev --routes server/pages --src server --output server/node_modules/@sapper -o",
    "chart:build-docs": "sapper build --routes server/pages --src server --output server/node_modules/@sapper",
    "chart:export-docs": "sapper export --routes server/pages --src server --output server/node_modules/@sapper --static server/static --basepath chart-module-my-chart-module && cp -R __sapper__/export/chart-module-my-chart-module/ docs/",
    "chart:preview-docs": "serve ./",
    "chart:build-lib": "rollup -c rollup.config.lib.js"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "d3": "^6.2.0",
    "d3-appendselect": "^0.0.4",
